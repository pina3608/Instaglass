<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  
  
   
    <servlet>
        <servlet-name>receivePicture</servlet-name>
        <servlet-class>com.instaglass.server.ReceivePictureServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>receivePicture</servlet-name>
        <url-pattern>/receivePicture</url-pattern>
    </servlet-mapping>
    
   
    <servlet>
        <servlet-name>sendPicture</servlet-name>
        <servlet-class>com.instaglass.server.SendPictureServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sendPicture</servlet-name>
        <url-pattern>/sendPicture</url-pattern>
    </servlet-mapping>
   
      <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>receivePicture.jsp</welcome-file>
  </welcome-file-list>
</web-app>